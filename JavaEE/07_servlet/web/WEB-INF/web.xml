<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
<!--    alt+Enter快捷键-->
    <servlet>
        <servlet-name>RequestAPIServlet</servlet-name>
        <servlet-class>com.ly.servlet.RequestAPIServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>RequestAPIServlet</servlet-name>
        <url-pattern>/req</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>com.ly.servlet.Servlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/s1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.ly.servlet.Servlet2</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/s2</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ForwardC</servlet-name>
        <servlet-class>com.ly.servlet.ForwardC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForwardC</servlet-name>
        <url-pattern>/fc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseIOServlet</servlet-name>
        <servlet-class>com.ly.servlet.ResponseIOServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ResponseIOServlet</servlet-name>
        <url-pattern>/resp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletResponse1</servlet-name>
        <servlet-class>com.ly.servlet.ServletResponse1</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>ServletResponse1</servlet-name>
        <url-pattern>/sr1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletResponse2</servlet-name>
        <servlet-class>com.ly.servlet.ServletResponse2</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>ServletResponse2</servlet-name>
        <url-pattern>/sr2</url-pattern>
    </servlet-mapping>
</web-app>